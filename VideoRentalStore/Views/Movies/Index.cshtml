@model VideoRentalStore.ViewModels.MoviesViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>

<div>
    <table>
        <tr>
            <th>Movie</th>
        </tr>
        @foreach (var movie in @Model.Movies)
        {
            <tr><td>@movie.Name</td></tr>
        }
    </table>
</div>